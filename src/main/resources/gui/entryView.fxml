            </GridPane>
         </center>
         <bottom>
            <GridPane alignment="CENTER_LEFT" prefHeight="43.0" prefWidth="600.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
               <children>
                  <Button fx:id="backButton" mnemonicParsing="false" onAction="#fullDataView" text="Back" GridPane.columnIndex="0">
                  </Button>
                  <HBox GridPane.columnIndex="1" spacing="5">
                     <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#editEntry" text="Delete"></Button>
                     <Button fx:id="editButton" mnemonicParsing="false" onAction="#editEntry" text="Edit"></Button>
                     <Button fx:id="saveButton" mnemonicParsing="false" onAction="#finishEdit" text="Cancel" visible="false" managed="false"></Button>
                     <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#saveEdit" text="Save" visible="false" managed="false"></Button>
                  </HBox>
               </children>
            </GridPane>
