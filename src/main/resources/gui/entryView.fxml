                        <CheckBox fx:id="domesticCheck" mnemonicParsing="false" text="Domestic Case" />
                     </children>
                  </VBox>
                  <VBox alignment="TOP_CENTER" prefHeight="266.0" prefWidth="201.0" GridPane.rowIndex="1">
                     <children>
                        <Label text="Case Number" />
